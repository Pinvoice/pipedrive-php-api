{
    "name": "pinvoice/pipedrive-php-api",
    "description": "PHP API for Pipedrive",
    "keywords": ["pipedrive", "api"],
    "homepage": "https://github.com/Pinvoice/pipedrive-php-api",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "curl/curl": "1.1.5",
        "zendframework/zend-filter": "2.3.3",
        "zendframework/zend-validator": "2.3.3"
    },
    "license": "MIT",
    "authors": [{
        "name": "Melvin Lammerts",
        "email": "melvin@melvin.sh"
    }],
    "autoload": {
        "psr-4": {
            "Pinvoice\\Pipedrive\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
